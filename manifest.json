{
    "name": "QR Code Generator",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Generates QR Code of any webpage or text.",
    "author": "Sudip Mahato",
    "action": {
        "default_popup": "index.html",
        "default_title": "QR Code Generator",
        "default_logo": "assets/logo_1.png"
    },
    "permissions": [
        "activeTab","tabs"
    ],
    "icons": {
        "128": "assets/logo_1.png"
    }
}